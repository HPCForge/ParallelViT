ninja_required_version = 1.3
cxx = c++
nvcc = /opt/apps/cuda/11.7.1/bin/nvcc

cflags = -pthread -Wno-unused-result -Wsign-compare -DNDEBUG -g -fwrapv -O3 -Wall -fPIC -I/dfs6/pub/sheikhh1/envs/sciml/lib/python3.10/site-packages/cutlass_library/source/include -I/dfs6/pub/sheikhh1/envs/sciml/lib/python3.10/site-packages/cutlass_library/source/tools/util/include -I/dfs6/pub/sheikhh1/envs/sciml/lib/python3.10/site-packages/torch/include -I/dfs6/pub/sheikhh1/envs/sciml/lib/python3.10/site-packages/torch/include/torch/csrc/api/include -I/dfs6/pub/sheikhh1/envs/sciml/lib/python3.10/site-packages/torch/include/TH -I/dfs6/pub/sheikhh1/envs/sciml/lib/python3.10/site-packages/torch/include/THC -I/opt/apps/cuda/11.7.1/include -I/dfs6/pub/sheikhh1/envs/sciml/include -I/opt/apps/python/3.10.2/include/python3.10 -c
post_cflags = -std=c++17 -DTORCH_API_INCLUDE_EXTENSION_H '-DPYBIND11_COMPILER_TYPE="_gcc"' '-DPYBIND11_STDLIB="_libstdcpp"' '-DPYBIND11_BUILD_ABI="_cxxabi1011"' -DTORCH_EXTENSION_NAME=linearGelu -D_GLIBCXX_USE_CXX11_ABI=0
cuda_cflags = -I/dfs6/pub/sheikhh1/envs/sciml/lib/python3.10/site-packages/cutlass_library/source/include -I/dfs6/pub/sheikhh1/envs/sciml/lib/python3.10/site-packages/cutlass_library/source/tools/util/include -I/dfs6/pub/sheikhh1/envs/sciml/lib/python3.10/site-packages/torch/include -I/dfs6/pub/sheikhh1/envs/sciml/lib/python3.10/site-packages/torch/include/torch/csrc/api/include -I/dfs6/pub/sheikhh1/envs/sciml/lib/python3.10/site-packages/torch/include/TH -I/dfs6/pub/sheikhh1/envs/sciml/lib/python3.10/site-packages/torch/include/THC -I/opt/apps/cuda/11.7.1/include -I/dfs6/pub/sheikhh1/envs/sciml/include -I/opt/apps/python/3.10.2/include/python3.10 -c
cuda_post_cflags = -D__CUDA_NO_HALF_OPERATORS__ -D__CUDA_NO_HALF_CONVERSIONS__ -D__CUDA_NO_BFLOAT16_CONVERSIONS__ -D__CUDA_NO_HALF2_OPERATORS__ --expt-relaxed-constexpr --compiler-options ''"'"'-fPIC'"'"'' -std=c++17 -DTORCH_API_INCLUDE_EXTENSION_H '-DPYBIND11_COMPILER_TYPE="_gcc"' '-DPYBIND11_STDLIB="_libstdcpp"' '-DPYBIND11_BUILD_ABI="_cxxabi1011"' -DTORCH_EXTENSION_NAME=linearGelu -D_GLIBCXX_USE_CXX11_ABI=0 -gencode=arch=compute_80,code=sm_80
cuda_dlink_post_cflags = 
ldflags = 

rule compile
  command = $cxx -MMD -MF $out.d $cflags -c $in -o $out $post_cflags
  depfile = $out.d
  deps = gcc

rule cuda_compile
  depfile = $out.d
  deps = gcc
  command = $nvcc  $cuda_cflags -c $in -o $out $cuda_post_cflags





build /dfs6/pub/sheikhh1/ParallelViT/linearGelu/build/temp.linux-x86_64-3.10/linearGelu.o: compile /dfs6/pub/sheikhh1/ParallelViT/linearGelu/linearGelu.cpp
build /dfs6/pub/sheikhh1/ParallelViT/linearGelu/build/temp.linux-x86_64-3.10/linearGelu_kernel.o: cuda_compile /dfs6/pub/sheikhh1/ParallelViT/linearGelu/linearGelu_kernel.cu







